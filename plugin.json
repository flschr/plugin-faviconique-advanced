{
  "version": "0.1.8",
  "title": "Faviconique+",
  "description": "Emoji favicon via EmojiCDN for all browsers.",
  "fields": [
    {
      "field": "params.faviconique_emoji",
      "label": "Emoji (only one)",
      "type": "string"
    },
    {
      "field": "params.faviconique_style",
      "label": "Emoji style",
      "type": "select",
      "options": ["apple", "twitter", "facebook", "google"]
    },
    {
      "field": "params.faviconique_title",
      "label": "Home screen title (optional)",
      "type": "string",
      "placeholder": ""
    },
    {
      "field": "params.faviconique_theme_color",
      "label": "Theme color for fullscreen browser app",
      "type": "color",
      "placeholder": "#000000"
    },
    {
      "field": "params.faviconique_webapp_capable",
      "label": "Enable standalone (full screen) app mode",
      "type": "boolean"
    }
  ]
}
