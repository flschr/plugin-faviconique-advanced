{
  "version": "0.2.1",
  "title": "Faviconique+",
  "description": "Emoji favicon via EmojiCDN with web app manifest for installs.",
  "fields": [
    {
      "field": "params.faviconique_emoji",
      "label": "Emoji (only one)",
      "type": "string"
    },
    {
      "field": "params.faviconique_style",
      "label": "Emoji style (apple / twitter / facebook / google)",
      "type": "string"
    },
    {
      "field": "params.faviconique_title",
      "label": "Home screen title (optional)",
      "type": "string",
      "placeholder": ""
    },
    {
      "field": "params.faviconique_theme_color",
      "label": "Theme color for browser UI",
      "type": "color",
      "placeholder": "#000000"
    },
    {
      "field": "params.faviconique_display",
      "label": "Web app display (standalone or browser)",
      "type": "string",
      "placeholder": "standalone"
    }
  ]
}
